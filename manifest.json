{
    "manifest_version": 3,
    "name": "YouTube URL Hyphenator",
    "version": "1.1",
    "description": "Redirects youtube.com URLs to yout-ube.com. Can be toggled via popup.",
    "permissions": [
      "webNavigation",
      "tabs",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {              
        "16": "icon16.png",
        "32": "icon32.png"            
      }
    },
    "icons": {                        
      "16": "icon16.png",             
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }